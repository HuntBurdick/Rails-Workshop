<%= render :partial => 'admin/template/form_errors', :object => @parent_item %>
<%= simple_form_for @parent_item, :url => url_for(:action => 'update'), :method => 'put', :html => { :role => "form" } do |form| %>

	<%= render :partial => 'form', :object => form %>
	
	<%= submit_tag 'Submit', :class => 'btn btn-primary' %>

<% end %>